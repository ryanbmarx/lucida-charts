{
  "name": "s2-app",
  "version": "1.0.0",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-multi-entry": "^3.0.0",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.1",
    "@turf/centroid": "^6.0.2",
    "@turf/turf": "^5.1.6",
    "autoprefixer": "^9.7.6",
    "chokidar": "^3.4.0",
    "colors": "^1.4.0",
    "cssnano": "^4.1.10",
    "directory-tree": "^2.2.4",
    "glob": "^7.1.6",
    "gulp-cli": "^2.2.0",
    "journalize": "^2.4.0",
    "lodash.debounce": "^4.0.8",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.27",
    "postcss-cli": "^7.1.0",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-livereload": "^1.2.0",
    "rollup-plugin-postcss": "^2.9.0",
    "rollup-plugin-svelte": "^5.1.1",
    "rollup-plugin-terser": "^5.3.0",
    "sander": "^0.6.0",
    "svelte": "^3.19.2",
    "topojson-client": "^3.1.0",
    "turf-polygon": "^1.0.3"
  },
  "dependencies": {
    "@newswire/frames": "^0.3.1",
    "d3": "^5.15.1",
    "highcharts": "^9.0.0",
    "mapbox-gl": "^1.9.1",
    "sirv-cli": "^0.4.5"
  },
  "scripts": {
    "autobuild": "rollup -c -w",
    "build": "mkdir -p public/ && run-p build:* && npm run build:index",
    "build:html": "node scripts/build-html.js",
    "build:index": "cp src/static/index.html public/index.html && node scripts/main-index.js",
    "build:js": "rollup -c",
    "build:static": "mkdir -p public/narratives && cp -rf src/static/* public/ ",
    "build:styles": "postcss src/css/*.css --dir public/css",
    "dev": ". .env && run-p start:dev autobuild watch:**",
    "start": "sirv public --single",
    "start:dev": "sirv public --single --dev",
    "watch:html": "node scripts/build-html.js --watch",
    "watch:styles": "postcss src/css/*.css --dir public/css --watch --verbose --map"
  },
  "browserslist": "> 0.25%, not dead"
}
